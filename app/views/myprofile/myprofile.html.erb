<div class="container">
	<div class="offset4"><img src="/assets/parabola-logo.png" class="img-polaroid"></div>
	<div class="offset4"><h4>Registration for Parabola Social Network</h4></div>
</br></br><br>
	<%= form_for @myprofile, :url =>{:controller => "myprofile",:action => "savemyprofile",:method => :post}, :html => {:multipart => true} do |f| %>
		<div class="row">
			<div class="span1 offset4"><%= label_tag 'Mobile' %></div>
			<%= f.text_field :mobile %>
		</div>
		<div class="row">
			<div class="span1 offset4"><%= label_tag 'Location' %></div>
			<%= f.text_field :location %>
		</div>
		<div class="row">
			<div class="span1 offset4"><%= label_tag 'Photo' %></div>
			<p><%= f.file_field :image %></p>
		</div>

		<div class="row">
			<div class="span3 offset5"><%= submit_tag 'Submit', :class => 'btn' %>&nbsp;<%= link_to 'Cancel', home_path, :class => 'btn' %></div>
		</div>
	<% end %>
</div>